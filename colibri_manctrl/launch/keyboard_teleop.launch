<launch>

  <node pkg="turtlebot_teleop" type="turtlebot_teleop_key" name="keyboard_teleop" output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="0.2" type="double"/>
    <remap from="keyboard_teleop/cmd_vel" to="t_cmd_vel" />
  </node>
  
</launch>
